# Local/private config goes in the include
[include]
    path = ~/.gitconfig.local
[alias]
    st = status
    com = commit
    commit-af = commit --amend --no-edit
[core]
    excludesfile = ~/.gitignore
    editor = vim
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[push]
    default = simple
